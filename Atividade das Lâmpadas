const fs = require("fs");
const input = fs.readFileSync(0, "utf8");

let linhas = input.trim().split('\n');
let n = Number(linhas[0]);
let comandos = linhas[1].trim().split(' ').map(Number);

let a = 0;
let b = 0;

for (let i = 0; i < n; i++) {
    if (comandos[i] == 1) {
        
        a = a == 1 ? 0 : 1;
    } else if (comandos[i] == 2) {
        
        a = a == 1 ? 0 : 1;
        b = b == 1 ? 0 : 1;
    }
}

console.log(a);
console.log(b);
