// Lendo a entrada do exercício
const fs = require("fs");
const input = fs.readFileSync(0, "utf8");

let p = parseInt(input.split(" ")[0])
let r = parseInt(input.split(" ")[1])

// Seu código vai aqui
if(p ==0){
    console.log("C");
}
if(p ==1 && r==0){
    console.log("B");
}
if(p == 1 && r ==1){
    console.log("A")
}
