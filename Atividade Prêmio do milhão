const fs = require("fs");
const input = fs.readFileSync(0, "utf8");
const lines = input.trim().split("\n");

const N = parseInt(lines[0]); // n√∫mero de dias
let soma = 0;
let dias = 0;

for (let i = 1; i <= N; i++) {
    const acessos = parseInt(lines[i]);
    soma += acessos;
    dias++;

    if (soma >= 1000000) {
        break;
    }
}

console.log(dias);
